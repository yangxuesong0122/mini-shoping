.home-swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.home-swiper swiper image {
  width: 100%;
}
.home-cate {
  display: flex;
  padding: 20rpx;
}
.home-cate navigator {
  flex: 1;
}
.home-cate navigator image {
  width: 100%;
}
.home-floor .floor-group .floor-title image {
  width: 100%;
}
.home-floor .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
  /*后四个超链接*/
}
.home-floor .floor-group .floor-list navigator:nth-last-child(-n+4) {
  /*第一张图片的高度
              原图的宽高 232 * 386
              232/386=33.33vw/height
              height: 33.33vw * 386 / 232;
            第二章图片的高度是第一张的一半
          */
  height: calc(33.33vw*386/232/2);
}
.home-floor .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
}
