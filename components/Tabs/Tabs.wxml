
<view class="tabs">
  <view class="tabs_title">
    <view
      bindtap="handleItemTap"
      wx:for="{{tabs}}"
      wx:key="id"
      data-index="{{index}}"
      class="tab_item {{item.isActive?'active':''}}">
      {{item.value}}
    </view>
  </view>
  <view class="tabs_content">
  </view>
</view>